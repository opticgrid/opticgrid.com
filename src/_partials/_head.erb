<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<% resource_title = strip_html(strip_newlines(title)) %>
<title>
  <% if resource_title != "Index" %><%= resource_title %> | <%= metadata.title
  %><% else %><%= metadata.title %><% end %>
</title>
<meta name="description" content="<%= metadata.description %>" />
<link rel="apple-touch-icon" sizes="180x180" href="<%= relative_url "/favicon/apple-touch-icon.png" %>" />
<link rel="icon" type="image/png" sizes="32x32" href="<%= relative_url "/favicon/favicon-32x32.png" %>" />
<link rel="icon" type="image/png" sizes="16x16" href="<%= relative_url "/favicon/favicon-16x16.png" %>" />
<link rel="manifest" href="<%= relative_url "/site.webmanifest" %>" />
<link rel="mask-icon" href="<%= relative_url "/favicon/safari-pinned-tab.svg" %>" color="#ef4136" />
<meta name="msapplication-TileColor" content="#ffffff" />
<meta name="theme-color" content="#ffffff" />
<link rel="stylesheet" href="<%= webpack_path :css %>" />
<script src="<%= webpack_path :js %>" defer></script>
<%= live_reload_dev_js %>
